
create database kasir;
use kasir;

create table user(
id int auto_increment primary key,
username varchar(20),
password varchar(20)
);

insert into user values(1,'admin','admin');

create table barang(
id int auto_increment primary key,
nama varchar(50),
harga int,
stok int
);

create table transaksi(
id int auto_increment primary key,
tanggal timestamp default current_timestamp,
total int
);
